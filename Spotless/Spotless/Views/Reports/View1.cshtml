                    <th>@Html.DisplayNameFor(model => model.TotalPowerGenerationL)</th>
                    <th>@Html.DisplayNameFor(model => model.TotalPowerGenerationH)</th>
                    <th>@Html.DisplayNameFor(model => model.insertDateTime)</th>
                    <th></th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <th>@Html.DisplayFor(modelItem => item.Device.DeviceGroup.title)</th>
                        <th>@Html.DisplayFor(modelItem => item.deviceNumber)</th>
                        <th>@Html.DisplayFor(modelItem => item.deviceId)</th>
