                        <th>@Html.DisplayFor(modelItem => item.Load3PowerL)</th>
                        <th>@Html.DisplayFor(modelItem => item.Load3PowerH)</th>
                        <th>@Html.DisplayFor(modelItem => item.BatteryTemp)</th>
