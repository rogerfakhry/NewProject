<script>
    $(function () {
        $(".k-grid-excel").click(function (e) {
            // Prevent the default behavior which will prompt the user to save the generated file.
            e.preventDefault();
            // Get the Excel file as a data URL.
            console.log('test');
            var dataURL = new kendo.ooxml.Workbook(e.workbook).toDataURL();
            // Strip the data URL prologue.
            var base64 = dataURL.split(";base64,")[1];
            // Post the base64 encoded content to the server which can save it.
            $.post("Device/Excel_Export_Save", {
                base64: base64,
                fileName: "ExcelExport.xlsx"
            }
            , function (data) {
                var url = 'data:application/vnd.ms-excel,' + encodeURIComponent(data);
                location.href = url;
                return false;
            });
        });
    });
    function BeautifulAlert(message) {
        $("#AlertMessages").html(message);
        $("#dialog").dialog({
            modal: true
        });
    }
</script>
<script>
    window.onload = function () {
        var app3 = new Vue({
            el: '#app-31',
            data: {
                seen: true,
                DeviceId: "",
                GroupId: "",
                spinner:false,
                spinner1:false,
                status: {}
            },
            methods: {
                ReadParameters: async function () {
                    this.spinner =true;
                    if(this.DeviceId == "") {
                        BeautifulAlert("Please Select a Device Id!");
                        return false;
                    }
                    await axios.get('@(Url.Content("~/controlDevice/GetDeviceParametersList/"))' + this.DeviceId)
                    .then(response => {this.status = response.data.data;BeautifulAlert(response.data.message);this.spinner = false;});
                    console.log(this.status);
                },
        WriteParametersOnDevice: async function () {
            this.spinner =true;
            if(this.DeviceId == "") {
                BeautifulAlert("Please Select a Device Id!");
                return false;
            }
            await axios.post('@(Url.Content("~/controlDevice/SaveDeviceParametersList/"))' + this.DeviceId,{"status":this.status})
            .then(response => {BeautifulAlert(response.data.message);this.spinner =false;});
            console.log(this.status);
        },
        WriteParametersOnStreet: async function () {
            this.spinner1 =true;
            if(this.GroupId == "") {
                BeautifulAlert("Please Select a Street!");
                return false;
            }
            await axios.post('@(Url.Content("~/controlDevice/SaveDeviceParametersListToGroup/"))' + this.GroupId,{"status":this.status})
            .then(response => {BeautifulAlert(response.data.message);this.spinner1 = false;});
            console.log(this.status);
        },
        button2_click : function(){
            this.status.button1 = !this.status.button2;
            console.log(this.status);
        },
        button4_click : function(){
            this.status.button3 = !this.status.button4;
            console.log(this.status);
        },
        button_click : function(value1,value2){
            this.status[value2] = !this.status[value1];
            console.log(this.status);
        },
        radioButtonChecked: function(value1,value2,value3,value4){

                this.status[value1] = false;
                this.status[value2] = false;
                this.status[value3] = false;
                this.status[value4] = true;
                console.log(this.status);

        }

    }
    })
    }
</script>

